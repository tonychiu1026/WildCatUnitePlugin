<UserControl x:Class="WildCatUnitePlugin.UI.ClientIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WildCatUnitePlugin.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Background="#FF423E4F">
            <Button x:Name="QuickAccessButton" Click="QuickAccessButton_Click">
                <Image x:Name="QuickAccessIconImage" Source="/WildCatUnitePlugin;component/img/icon.png" ToolTipService.ShowDuration="5000"/>
            </Button>
        </StackPanel>
    </Grid>
</UserControl>
